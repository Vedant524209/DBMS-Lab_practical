create database l;

use l;

CREATE TABLE Em (
    eid INT PRIMARY KEY,
    e_name VARCHAR(100),
    e_city VARCHAR(50),
    salary INT
);

INSERT INTO Em (eid, e_name, e_city, salary) VALUES
(1, 'Meera', 'Pune', 30000),
(2, 'Vedant', 'Mumbai', 55000),
(3, 'Rohan', 'Delhi', 42000),
(4, 'Maya', 'Pune', 20000),
(5, 'Manish', 'Mumbai', 50000);

select  * from em;

select * from em
where salary >30000;

SELECT * FROM Em WHERE e_city != 'Pune';

SELECT * FROM Em WHERE salary <= 30000;

select * from em
where e_city='mumbai' and salary>3000;

SELECT * FROM Em
WHERE e_name LIKE 'M%';

SELECT * FROM Em
WHERE e_name LIKE '%a';


